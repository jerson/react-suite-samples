{
    "name": "react-suite-samples",
    "version": "0.0.5",
    "license": "Apache-2.0",
    "scripts": {
        "start:web": "cd public && node server.js",
        "build:web": "NODE_ENV=production webpack --config webpack.config.prod.js",
        "start": "node node_modules/react-native/local-cli/cli.js start --skipflow",
        "android": "node node_modules/react-native/local-cli/cli.js run-android",
        "ios": "node node_modules/react-native/local-cli/cli.js run-ios",
        "prebuild": "node node_modules/.bin/rimraf build",
        "build": "npm run tslint && tsc",
        "tslint": "tslint --project tsconfig.json -r tslint.json -r ./node_modules/tslint-microsoft-contrib --fix || true",
        "pretest": "npm run build",
        "format": "npm run prettier && npm run tslint",
        "prettier": "prettier --write --single-quote 'src/**/*.tsx'",
        "precommit": "lint-staged",
        "test": "jest",
        "build:android": "cd android && ./gradlew assembleRelease",
        "push:android": "code-push release-react ReactSuiteAndroid android -d Production",
        "push:ios": "code-push release-react ReactSuiteIOS ios -d Production --plistFile 'ios/ReactSuiteSamples/Info.plist'",
        "push": "npm run push:android && npm run push:ios"
    },
    "lint-staged": {
        "*.tsx": [
            "npm run prettier",
            "npm run tslint",
            "git add"
        ]
    },
    "dependencies": {
        "pouchdb-browser": "^6.2.0",
        "prop-types": "^15.5.10",
        "react": "^15.4.1",
        "react-dom": "^15.4.1",
        "react-native": "^0.42.0",
        "react-native-blur": "^3.0.0-alpha",
        "react-native-code-push": "^1.17.4-beta",
        "react-native-device-info": "^0.10.2",
        "react-native-drawer": "^2.3.0",
        "react-native-google-analytics": "^1.3.2",
        "react-native-linear-gradient": "^2.0.0",
        "react-native-orientation": "git+https://github.com/Dudyboy/react-native-orientation.git",
        "react-native-shared-preferences": "^0.0.8",
        "react-native-slider": "^0.10.0",
        "react-native-syntax-highlighter": "^1.2.1",
        "react-native-vector-icons": "^4.2.0",
        "react-native-web": "^0.0.96",
        "react-router-dom": "^4.1.1",
        "react-router-native": "^4.1.1",
        "react-suite": "^0.0.135-dev",
        "realm": "^1.3.1"
    },
    "devDependencies": {
        "@types/jest": "^19.2.3",
        "@types/react": "^15.0.25",
        "@types/react-native": "^0.44.7",
        "@types/react-test-renderer": "^15.5.0",
        "babel-core": "^6.24.1",
        "babel-eslint": "7.2.3",
        "babel-jest": "^20.0.3",
        "babel-loader": "^7.0.0",
        "babel-plugin-import": "^1.2.1",
        "babel-polyfill": "^6.23.0",
        "babel-preset-es2015": "^6.24.1",
        "babel-preset-react": "^6.24.1",
        "babel-preset-react-app": "^3.0.0",
        "babel-preset-react-native": "^1.9.2",
        "babel-preset-stage-0": "^6.24.1",
        "case-sensitive-paths-webpack-plugin": "2.1.1",
        "chalk": "1.1.3",
        "connect-history-api-fallback": "1.3.0",
        "copy-webpack-plugin": "^4.0.1",
        "cross-spawn": "5.1.0",
        "file-loader": "0.11.1",
        "filesize": "^3.5.10",
        "font-loader": "^0.1.2",
        "fs-extra": "3.0.1",
        "gzip-size": "3.0.0",
        "html-webpack-plugin": "2.28.0",
        "husky": "^0.13.4",
        "jest": "20.0.4",
        "lint-staged": "^3.6.0",
        "opener": "^1.4.3",
        "path-exists": "3.0.0",
        "prettier": "^1.4.1",
        "react-dev-utils": "^3.0.0",
        "react-hot-loader": "^3.0.0-beta.7",
        "react-test-renderer": "16.0.0-alpha.6",
        "rimraf": "^2.6.1",
        "swig-webpack-plugin": "^0.5.1",
        "ts-jest": "^20.0.5",
        "tslint": "^5.4.0",
        "tslint-microsoft-contrib": "^5.0.0",
        "tsutils": "^2.3.0",
        "typescript": "^2.3.4",
        "webpack": "2.6.1",
        "webpack-closure-compiler": "^2.1.4",
        "webpack-dev-server": "2.4.5",
        "webpack-manifest-plugin": "1.1.0"
    },
    "jest": {
        "preset": "react-native",
        "transform": {
            "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
            ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
        },
        "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js"
        ]
    }
}
